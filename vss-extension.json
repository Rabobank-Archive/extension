{
    "manifestVersion": 1,
    "version": "1.0.3",
    "publisher": "riezebosch",
    "public": false,
    "galleryFlags": [
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Boards"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "id": "azure-devops-compliance",
    "name": "Azure DevOps Compliance",
    "files": [
        {
            "path": "build",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "feature-compliance",
            "type": "ms.vss-web.feature",
            "description": "Enable the  Compliancy hub group",
            "targets": [
                "ms.vss-web.managed-features",
                "ms.vss-web.managed-features-onprem"
            ],
            "properties": {
                "name": "Compliancy",
                "userConfigurable": true,
                "hostConfigurable": true
            }
        },
        {
            "id": "compliance",
            "type": "ms.vss-web.hub-group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Compliancy",
                "icon": "build/logo.png"
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "riezebosch.azure-devops-compliance.feature-compliance"
                    }
                }
            ]
        },
        {
            "id": "repos",
            "type": "ms.vss-web.hub",
            "targets": [
                ".compliance"
            ],
            "properties": {
                "name": "Repos",
                "iconName": "OpenSource",
                "order": 1,
                "uri": "build/index.html#repositories"
            }
        },
        {
            "id": "buildpipelines",
            "type": "ms.vss-web.hub",
            "targets": [
                ".compliance"
            ],
            "properties": {
                "name": "Pipelines",
                "iconName": "Build",
                "order": 2,
                "uri": "build/index.html#build-pipelines"
            }
        },
        {
            "id": "releasepipelines",
            "type": "ms.vss-web.hub",
            "targets": [
                ".compliance"
            ],
            "properties": {
                "name": "Releases",
                "iconName": "Rocket",
                "order": 3,
                "uri": "build/index.html#release-pipelines"
            }
        },
        {
            "id": "overview",
            "type": "ms.vss-web.hub",
            "targets": [
                ".compliance"
            ],
            "properties": {
                "name": "Overview",
                "iconName": "Repo",
                "order": 0,
                "uri": "build/index.html#overview"
            }
        }
    ]
}